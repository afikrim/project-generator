{
  "name": "name",
  "description": "{{ description }}",
  "version": "0.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "babel ./src --out-dir dist --source-maps --no-comments --minified",
    "build:watch": "babel ./src --out-dir dist --watch",
    "clean": "rm -rf ./dist",
    "dev": "concurrently \"npm:build:watch\" \"npm:development\"",
    "development": "nodemon ./dist/index",
    "prestart": "npm run clean && npm run build",
    "start": "node ./dist/index"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "babel-plugin-module-resolver": "^4.0.0",
    "concurrently": "^5.3.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "morgan": "^1.10.0"
  }
}
